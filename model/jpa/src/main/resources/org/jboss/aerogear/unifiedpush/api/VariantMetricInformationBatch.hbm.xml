<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.jboss.aerogear.unifiedpush.api.VariantMetricInformationBatch" table="variant_metric_info_batch">
        <id name="id" type="java.lang.String">
            <column name="id" />
            <generator class="assigned" />
        </id>
        <property name="variantID" type="java.lang.String" index="variant_idx">
            <column name="variant_id" not-null="true"/>
        </property>
        <property name="pushMessageInformationID" type="java.lang.String" index="pmi_idx">
            <column name="push_message_info_id" not-null="true"/>
        </property>
        <property name="receivers" type="java.lang.Long" index="receivers_idx">
            <column name="receivers" />
        </property>
        <property name="deliveryStatus" type="java.lang.Boolean" index="delivery_status_idx">
            <column name="delivery_status" />
        </property>
        <property name="reason" type="java.lang.String" index="reason_idx">
            <column name="reason" />
        </property>
    </class>
</hibernate-mapping>
